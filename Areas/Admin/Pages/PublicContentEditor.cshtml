<script>

    $(document).ready(function () {
        // Get the selected datetime value when the datepicker value changes
        $('#kt_datetimepicker_8').on('change.datetimepicker', function (e) {
            var selectedDatetime = e.date.format('YYYY-MM-DD HH:mm:ss');

            // Update the Model.CountDownSections.CountDown property
            // Replace '#CountDownInput' with the actual ID or class of your input element
            $('#CountDown').val(selectedDatetime);
        });
    });

    function editButtonClickSlider(PublicSliderId) {
        $.ajax({
            type: "GET",
            beforeSend: function (xhr) {
                xhr.setRequestHeader("XSRF-TOKEN",
                    $('input:hidden[name="__RequestVerificationToken"]').val());
            }
            ,
            url: "/Admin/PublicContentEditor?handler=SingleSliderForEdit",

            data: { "PublicSliderId": PublicSliderId },
            contentType: "application/json",
            dataType: "json",
            success: function (response) {
                
                document.getElementById("HiddenIdSlider").value = response.PublicSliderId
                console.log(document.getElementById("HiddenIdSlider").value)
                document.getElementById("BackgroundId").value = response.Background
                document.getElementById("Title1ArID").value = response.Title1Ar
                document.getElementById("TitleEn1ID").value = response.Title1En
                document.getElementById("Title2ArID").value = response.Title2Ar
                document.getElementById("Title2EnID").value = response.Title2En
                document.getElementById("DescriptionArID").value = response.DescriptionAr
                document.getElementById("DescriptionEnID").value = response.DescriptionEn
                document.getElementById("EIsActiveId").checked = response.IsImage
                console.log(response);
                $('#showEditModal').modal('show');
            },

            failure: function (response) {
                alert(response);
            }
        });
    }
    
